{
    "name": "kiwi-suite/application-console",
    "description": "kiwi-suite/application-console",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "require": {
        "php": "^7.1",
        "kiwi-suite/application": "^0.1.1",
        "kiwi-suite/config": "^0.1.1",
        "symfony/console": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.5",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "KiwiSuite\\ApplicationConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KiwiSuiteMisc\\ApplicationConsole\\": "tests/misc/",
            "KiwiSuiteTest\\ApplicationConsole\\": "tests/src/"
        }
